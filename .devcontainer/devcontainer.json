{
  "name": "Robot Framework GUI",
  "image": "consol/ubuntu-xfce-vnc:latest",
  "features": {
    "ghcr.io/devcontainers/features/node:1": {
      "version": "lts"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "d-biehl.robotcode"
      ]
    }
  },
  "postCreateCommand": "bash .devcontainer/setup.sh",
  "forwardPorts": [6080],
  "remoteUser": "root"
}
